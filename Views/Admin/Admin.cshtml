@using Fpm.MainUI
@model Fpm.MainUI.Models.AdminModel

@{ Layout = "~/Views/Shared/_LayoutPage.cshtml"; }
@{
    ViewBag.Title = "FPM - Admin";
}

<script src="@(AppConfig.JsPath)PageAdmin.js" type="text/javascript"></script>

<div class="standardWidth admin">
    <h2 class="subheading">Admin - @Model.AdminType</h2>
    @Html.Partial("_AdminSidebar")
    <div class="clearfix" style="float:left;">
        @Html.Partial("_DeleteIndicator")
        @Html.Partial("_ChangeIndicatorOwner")
    </div>
</div>
